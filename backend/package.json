{
  "name": "college-platform-backend",
  "version": "1.0.0",
  "description": "Backend API for College Platform - Enterprise course management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node seeders/seed.js",
    "db:migrate": "node migrations/migrate.js",
    "test:data:create": "node scripts/test-data.js",
    "test:data:update": "node scripts/test-data.js update",
    "vercel-build": "chmod +x ./vercel-build.sh && ./vercel-build.sh",
    "db:ensure": "node scripts/ensure-database-schema.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "sequelize",
    "college",
    "education",
    "course-management"
  ],
  "author": "College Platform Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@mui/lab": "^7.0.0-beta.17",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
